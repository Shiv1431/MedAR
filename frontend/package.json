{
  "name": "frontend",
  "private": true,
  "proxy": "https://localhost:4400",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "npm install && vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "install:clean": "if exist node_modules rmdir /s /q node_modules && npm install",
    "dev:prod": "vite --mode production",
    "build:prod": "vite build --mode production",
    "build:dev": "vite build --mode development",
    "serve": "serve -s dist",
    "setup": "npm install && npm run build",
    "setup:clean": "if exist node_modules rmdir /s /q node_modules && if exist dist rmdir /s /q dist && npm install && npm run build"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@material-tailwind/react": "^2.1.9",
    "@mui/icons-material": "^7.0.1",
    "@mui/material": "^7.0.1",
    "@react-three/drei": "^9.97.0",
    "@react-three/fiber": "^8.15.16",
    "axios": "^1.6.8",
    "date-fns": "^3.6.0",
    "framer-motion": "^11.0.3",
    "razorpay": "^2.9.3",
    "react": "^18.2.0",
    "react-datepicker": "^7.1.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.0.1",
    "react-loader-spinner": "^6.1.6",
    "react-router-dom": "^6.22.0",
    "three": "^0.159.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "vite": "^6.3.2",
    "serve": "^14.2.1"
  }
}
